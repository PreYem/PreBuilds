{
  "compilerOptions": {
    "target": "ES2017",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "ES2017"]
  },
    "include": ["src", "images.d.ts", "src/**/*.tsx" , "src/**/*.ts"]
}
